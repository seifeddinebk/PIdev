<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        <link rel="shortcut icon" href="img/logos/favicon.png">
        <link rel="apple-touch-icon" href="img/logos/apple-touch-icon-57x57.png">
        <link rel="apple-touch-icon" sizes="72x72" href="img/logos/apple-touch-icon-72x72.png">
        <link rel="apple-touch-icon" sizes="114x114" href="img/logos/apple-touch-icon-114x114.png">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}

        {% block stylesheets %}
             <link rel="stylesheet" href="{{asset('assets/client/css/plugins.css')}}">
             <link rel="stylesheet" href="{{asset('assets/client/quform/css/base.css')}}">
             <link href="{{asset('assets/client/css/styles.css')}}" rel="stylesheet">
        {% endblock %}

        {% block javascripts %}
            <script src="{{asset('assets/client/js/jquery.min.js')}}"></script>
            <script src="{{asset('assets/client/js/popper.min.js')}}"></script>
            <script src="{{asset('assets/client/js/bootstrap.min.js')}}"></script>
            <script src="{{asset('assets/client/js/core.min.js')}}"></script>
            <script src="{{asset('assets/client/search/search.js')}}"></script>
            <script src="{{asset('assets/client/js/main.js')}}"></script>
            <script src="{{asset('assets/client/quform/js/plugins.js')}}"></script>
            <script src="{{asset('assets/client/quform/js/scripts.js')}}"></script>



        {% endblock %}
    </head>
    <body>
        {{ include('client/header.html.twig') }}
        {% block body %}{% endblock %}
        {{ include('client/footer.html.twig') }}
    </body>
</html>
